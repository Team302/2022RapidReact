<?xml version="1.0"?>
<!DOCTYPE statedata SYSTEM "stateData.dtd">
<statedata>
	<controlData identifier="openloop" 
	             mode="PERCENT_OUTPUT"/>

	<controlData identifier="closedloop"
				 mode="VELOCITY_RPS"
				 proportional="0.155"
				 feedforward="0.045"/>
				 
	<mechanismTarget stateIdentifier="SHOOTEROFF"
	                 controlDataIdentifier="openloop"
					 value="0.0"/>

 <mechanismTarget stateIdentifier="SHOOTERCLOSE"
	                 controlDataIdentifier="closedloop"
					 value="40"
					 controlDataIdentifier2="openloop"
					 secondValue="-1.0"/>


 <mechanismTarget stateIdentifier="SHOOTERFAR"
	                 controlDataIdentifier="closedloop"
					 value="40"
					 controlDataIdentifier2="openloop"
					 secondValue="1.0"/>

<mechanismTarget stateIdentifier="SHOOTERCLOSELOW"
	                 controlDataIdentifier="closedloop"
					 value="10"
					 controlDataIdentifier2="openloop"
					 secondValue="1.0"/>					 

<mechanismTarget stateIdentifier="SHOOTERMANUAL"
	                 controlDataIdentifier="closedloop"
					 value="40"
					 controlDataIdentifier2="openloop"
					 secondValue="0.0"/>

